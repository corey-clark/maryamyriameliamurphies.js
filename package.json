  "license": "ISC",
  "homepage": "https://github.com/sjsyrek/maryamyriameliamurphies.js#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/sjsyrek/maryamyriameliamurphies.js.git"
  },
  "bugs": {
    "url": "https://github.com/sjsyrek/maryamyriameliamurphies.js/issues"
  },
  "keywords": [
    "javascript",
    "babel",
    "es6",
    "es2015",
    "haskell",
    "functional",
